int firstMissingPositive(vector<int>& nums) {
    //   int n=nums.size();
    //   int N=n+1;
    //     int total=N*(N+1)/2;
    //     for(int i=0;i<n;i++)
    //     {
    //         total -=nums[i];
    //     }
    //     return total;
    sort(nums.begin(),nums.end());
    int temp=1;
    int size=nums.size();
    for(int i=0;i<size;i++)
    {
        int ans=nums[i];
        if(ans==temp)
        {
            temp++;
        }
        
    }
    return temp;
    }
